<script>
  function mobile_open_nav(section) {
    s = document.getElementById("mobile_" + section + "_drop")
    h = document.getElementById("mobile_" + section + "_nav")
    a = document.getElementById(section + "_arrow")
    down = "/assets/buttons/ArrowDown.png"
    up = "/assets/buttons/ArrowUp.png"
    var t = 100
    var t2 = 200
    if (s.style.display == "none" || s.style.display == "") {
      a.src = up;
      $("#mobile_" + section + "_drop").show(t);
      h.style.backgroundColor = "#5b4a42";
      if (section == "create") {
        document.getElementById("shop_arrow").src = down;
        $("#mobile_shop_drop").hide(t2);
        document.getElementById("mobile_shop_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("connect_arrow").src = down;
        $("#mobile_connect_drop").hide(t2);
        document.getElementById("mobile_connect_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("studio_arrow").src = down;
        $("#mobile_studio_drop").hide(t2);
        document.getElementById("mobile_studio_nav").style.backgroundColor = "#aba7a3";
      }
      else if (section == "shop") {
        document.getElementById("create_arrow").src = down;
        $("#mobile_create_drop").hide(t2);
        document.getElementById("mobile_create_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("connect_arrow").src = down;
        $("#mobile_connect_drop").hide(t2);
        document.getElementById("mobile_connect_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("studio_arrow").src = down;
        $("#mobile_studio_drop").hide(t2);
        document.getElementById("mobile_studio_nav").style.backgroundColor = "#aba7a3";
      }
      else if (section == "connect") {
        document.getElementById("create_arrow").src = down;
        $("#mobile_create_drop").hide(t2);
        document.getElementById("mobile_create_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("shop_arrow").src = down;
        $("#mobile_shop_drop").hide(t2);
        document.getElementById("mobile_shop_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("studio_arrow").src = down;
        $("#mobile_studio_drop").hide(t2);
        document.getElementById("mobile_studio_nav").style.backgroundColor = "#aba7a3";
      }
      else if (section == "studio") {
        document.getElementById("create_arrow").src = down;
        $("#mobile_create_drop").hide(t2);
        document.getElementById("mobile_create_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("shop_arrow").src = down;
        $("#mobile_shop_drop").hide(t2);
        document.getElementById("mobile_shop_nav").style.backgroundColor = "#aba7a3";
        document.getElementById("connect_arrow").src = down;
        $("#mobile_connect_drop").hide(t2);
        document.getElementById("mobile_connect_nav").style.backgroundColor = "#aba7a3";
      }
    }
    else {
      a.src = down;
      $("#mobile_" + section + "_drop").hide(t2);
      h.style.backgroundColor = "#aba7a3";
    }
  }
  
</script>

