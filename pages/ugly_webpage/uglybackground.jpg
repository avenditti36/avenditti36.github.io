<script>
    //choosen for multi selects & sort
    $('#application_multi_select').chosen({
      width:"23em",
      disable_search: true,
      max_selected_options: 11,
    });
    
    $('#attribute_multi_select').chosen({
      width:"23em",
      disable_search: true,
      max_selected_options: 5,
    });
    
    $('#sort').chosen({
      width:"23em",
      disable_search: true,
      max_selected_options: 1,
    });
  
  $('#application_multi_select').change(function() {
    ga('send', 'event', 'help me choose', 'click', 'application select');
    if (screen.width > 467) {
      submitForm();
    }  
  });
  $('#attribute_multi_select').change(function() {
    ga('send', 'event', 'help me choose', 'click', 'attribute select');
    if (screen.width > 467) {
      submitForm();
    }
  });
  $('#sort').change(function() {
    ga('send', 'event', 'help me choose', 'click', 'sort');
    if (screen.width > 467) {
      submitForm();
    }
  });
    
  
  //clicking the being button hides and shows things to create a different view
  $("#begin_button").click(function() {
    goWithButton();
  });
  
  function goWithButton() {
    $('#copy').hide(); 
    $('#h2').hide(); 
    $('#outer_filters_div').css("background-color", "#E8E8E1");
    $('#begin_button').hide();
    $('#clear_filters1').hide();
    $('#clear_filters2').show();
    if (screen.width < 467) {
      $("#outer_filters_div").hide();
      $(".filter_button").text("Show Filters");
    }
    $('#filter_form').submit();
  };
  
  //clearing all filters visually removes them from chosen and submits the form again
  function clearAllFilters() {
    $('#application_multi_select option:selected').removeAttr('selected');
    $('#application_multi_select').trigger('chosen:updated');
    $('#attribute_multi_select option:selected').removeAttr('selected');
    $('#attribute_multi_select').trigger('chosen:updated');
    $('#filter_form').submit();
  };
  
  //this submits the form for on change of the chosen forms unless we are at the beginning screen
  //then the user must click the begin button
  function submitForm() {
    if ($("#begin_button").css('display') == 'none') {
      $('#filter_form').submit();
    }
  }
</script>

