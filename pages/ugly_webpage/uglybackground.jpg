  function mobile_selected_name(search){
    document.getElementById('mobile_filter').value = search
    if (search == "gift wrap") {
    document.getElementById('mobile_shop').value = "gift_wrap"
      }
    else {
      document.getElementById('mobile_shop').value = search
    }
    search_text = "search " + search
    str = search.toLowerCase().replace(/\b[a-z]/g, function(letter) {
        return letter.toUpperCase();
    });
    $('#mobile_search_selected_name').html(str + ":");

  };
    
  function mobile_selected_form(search){
    if (search == "collections") {
      document.mobile_search_form.action = "/spelunks"
    }
    else if (search == "designers") {
      document.mobile_search_form.action = "/spelunks"
    }
    else {
      document.mobile_search_form.action = "/shop"
    }
  };
  
  function mobile_fixH(one,two) {
    if (document.getElementById(one)) {
      var rh=document.getElementById(two).style.width
      rh = rh.replace('%','');
      if (rh == 0) {
        rh=55;
      }
      var nh = Math.max(79 - rh);
      document.getElementById(one).style.width=nh+"%";
    }
  }
  
    
    window.onload = setUp();
    
    function setUp(){
        document.getElementById('mobile_mag').style.width = '35%'; 
        mobile_selected_name('fabric'); 
